{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es5", "es2015", "es2016",
      "dom"
    ]
  },
  "files": [
    "json-normalizer.ts"
  ],
  "include": [
    "node_modules/@angular-devkit/core/node/*.ts",
    "./sha1/sha1.js",
    "./crypt/crypt.js",
    "./charenc.js"
  ]
}
