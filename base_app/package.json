{
  "name": "simpleui",
  "version": "0.1.0",
  "description": "base_app is the client component of the simpleui web ui library.",
  "keywords": [
    "simpleui",
    "client",
    "typescript"
  ],
  "author": "Jim Scarsdale",
  "license": "ISC",
  "scripts": {
    "ng": "ng",
    "build-client-dev": "bash make-version-txt.bash build-client-only && rm -rf dist && ng build --output-hashing all --extract-css --source-map && sh post-build-step.sh --include-mocks",
    "build-client-prod": "bash make-version-txt.bash build-client-prod && rm -rf dist && ng build --aot --output-hashing all --extract-css --no-source-map --no-named-chunks && sh post-build-step.sh --include-mocks",
    "copy-dist-to-output": "mkdir -p ../../../output/simpleui/base_app/ && cp dist.tgz ../../../output/simpleui/base_app/",
    "start-client-dev": "ng serve",
    "start-client-prod": "ng serve --aot",
    "post-build-step": "sh post-build-step.sh --include-mocks",
    "post-build-step--setup-bms-for-dev": "sh build-app-for-dev.sh 0 bms git-workspace-all ${USER}",
    "test": "ng test",
    "unit-test-nodejs-javascript": "bash src/test/normalizeJSON.unit-test.bash",
    "unit-test-simpleui-server": "bash src/public/simpleui-server/test/__simpleui-server.unit-test.bash",
    "lint--not-tested": "ng lint",
    "e2e--not-tested": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "12.2.16",
    "@angular/compiler": "^12.2.0-next",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@babel/core": "^7.0.0-bridge.0",
    "@babel/polyfill": "^7.6.0",
    "babel-loader": "^7.1.5",
    "clean-webpack-plugin": "^1.0.1",
    "core-js": "^3.21.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "find-process": "^1.4.2",
    "hammerjs": "^2.0.8",
    "jsontoxml": "^1.0.1",
    "microtime": "^3.0.0",
    "moment": "^2.24.0",
    "ng-packagr": "^12.2.6",
    "ngx-socket-io": "^4.1.0",
    "request": "^2.88.0",
    "rxjs": "6.5.3",
    "rxjs-compat": "^6.6.7",
    "sha1": "^1.1.1",
    "tailwindcss": "^2.0.0",
    "then-request": "^6.0.2",
    "tslib": "^2.1.0",
    "zeromq": "^5.2.8",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.16",
    "@angular/cli": "^12.2.16",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/language-service": "^12.2.16",
    "@angular/localize": "^12.2.16",
    "@angular/service-worker": "^12.2.16",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.0.0-bridge.0",
    "@babel/plugin-syntax-dynamic-import": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@swc/core": "^1.2.140",
    "@swc/wasm": "^1.2.140",
    "@types/express": "4.17.13",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.11.7",
    "codelyzer": "^6.0.2",
    "fast-xml-parser": "^3.21.1",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^7.0.0",
    "serve-static": "^1.14.2",
    "ts-node": "^7.0.1",
    "tslint": "~6.1.0",
    "tslint-loader": "^3.5.4",
    "typescript": "4.2.3",
    "webpack": "4.0.0",
    "webpack-cli": "3.3.4"
  }
}
