{
  "name": "ng-simple-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build-client-only": "bash make-version-txt.bash build-client-only && ng build --output-hashing all --extract-css --source-map && sh post-build-step.sh --include-mocks",
    "build-client-dev": "bash make-version-txt.bash build-client-dev && rm -rf dist && ng build --output-hashing all --extract-css --source-map && sh build-json-normalizer.sh && sh build-simpleui-server.bash && sh post-build-step.sh --include-mocks",
    "build-client-prod": "bash make-version-txt.bash build-client-prod && rm -rf dist && ng build --aot --output-hashing all --extract-css --no-source-map --no-named-chunks && sh build-json-normalizer.sh && sh -x build-simpleui-server.bash && sh post-build-step.sh --include-mocks",
    "build-json-normalizer": "sh -x build-json-normalizer.sh",
    "build-server": "sh -x build-simpleui-server.bash",
    "start-client-dev": "ng serve",
    "start-client-prod": "ng serve --aot",
    "start-server": "node dist/simpleui-server/simpleui-server.js --appName=bms --uiProp=ui.nodejs",
    "post-build-step": "sh post-build-step.sh --include-mocks",
    "post-build-step--setup-bms-for-dev": "sh build-app-for-dev.sh 0 bms git-workspace-all ${USER}",
    "test": "ng test",
    "unit-test-nodejs-javascript": "bash src/test/normalizeJSON.unit-test.bash",
    "unit-test-simpleui-server": "bash src/public/simpleui-server/test/__simpleui-server.unit-test.bash",
    "lint--not-tested": "ng lint",
    "e2e--not-tested": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@babel/polyfill": "^7.6.0",
    "babel-loader": "^7.1.5",
    "clean-webpack-plugin": "^1.0.1",
    "core-js": "^2.6.10",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "find-process": "^1.4.2",
    "hammerjs": "^2.0.8",
    "jsontoxml": "^1.0.1",
    "microtime": "^3.0.0",
    "moment": "^2.24.0",
    "ngx-socket-io": "^3.0.1",
    "request": "^2.88.0",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "sha1": "^1.1.1",
    "then-request": "^6.0.2",
    "tslib": "^1.10.0",
    "zeromq": "^4.6.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@types/express": "^4.17.1",
    "@types/jasmine": "2.8.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.3",
    "babel-core": "^7.0.0-bridge.0",
    "codelyzer": "~4.5.0",
    "fast-xml-parser": "^3.14.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "serve-static": "^1.14.1",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "3.2.4",
    "webpack-cli": "3.3.4"
  }
}
