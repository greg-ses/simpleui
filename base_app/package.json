{
  "name": "simpleui",
  "version": "0.1.0",
  "description": "base_app is the client component of the simpleui web ui library.",
  "keywords": [
    "simpleui",
    "client",
    "typescript"
  ],
  "author": "Jim Scarsdale",
  "license": "ISC",
  "scripts": {
    "ng": "ng",
    "build-client-dev": "bash make-version-txt.bash build-client-only && rm -rf dist && ng build --output-hashing all --extract-css --source-map && sh post-build-step.sh --include-mocks",
    "build-client-prod": "bash make-version-txt.bash build-client-prod && rm -rf dist && ng build --aot --output-hashing all --extract-css --no-source-map --no-named-chunks && sh post-build-step.sh --include-mocks",
    "copy-dist-to-output": "mkdir -p ../../../output/simpleui/base_app/ && cp dist.tgz ../../../output/simpleui/base_app/",
    "start-client-dev": "ng serve",
    "start-client-prod": "ng serve --aot",
    "post-build-step": "sh post-build-step.sh --include-mocks",
    "post-build-step--setup-bms-for-dev": "sh build-app-for-dev.sh 0 bms git-workspace-all ${USER}",
    "test": "ng test",
    "unit-test-nodejs-javascript": "bash src/test/normalizeJSON.unit-test.bash",
    "unit-test-simpleui-server": "bash src/public/simpleui-server/test/__simpleui-server.unit-test.bash",
    "lint--not-tested": "ng lint",
    "e2e--not-tested": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/cdk": "^8.2.1",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@angular/forms": "^9.1.13",
    "@angular/material": "8.2.1",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/router": "^9.1.13",
    "@babel/core": "^7.0.0-bridge.0",
    "@babel/polyfill": "^7.6.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.1",
    "core-js": "^2.6.10",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "find-process": "^1.4.2",
    "hammerjs": "^2.0.8",
    "jsontoxml": "^1.0.1",
    "microtime": "^3.0.0",
    "moment": "^2.24.0",
    "ng-packagr": "^9.0.0",
    "ngx-socket-io": "^3.0.1",
    "request": "^2.88.0",
    "rxjs": "^6.6.7",
    "sha1": "^1.1.1",
    "then-request": "^6.0.2",
    "tslib": "^1.10.0",
    "zeromq": "^6.0.0-beta.6",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular/cli": "^9.1.15",
    "@angular/compiler-cli": "^9.1.13",
    "@angular/language-service": "^9.1.13",
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@types/express": "^4.17.1",
    "@types/jasmine": "2.8.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "babel-core": "^7.0.0-bridge.0",
    "codelyzer": "^5.1.2",
    "fast-xml-parser": "^3.14.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "serve-static": "^1.14.1",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "3.8.3",
    "webpack": "4.0.0",
    "webpack-cli": "3.3.4"
  }
}