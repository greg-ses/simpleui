{
  "name": "simpleui",
  "version": "1.0.1",
  "description": "base_app is the client component of the simpleui web ui library.",
  "keywords": [
    "simpleui",
    "client",
    "typescript"
  ],
  "author": "Jim Scarsdale",
  "license": "ISC",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "build-client-dev": "bash make-version-txt.bash build-client-only && rm -rf dist dist.tgz && ng build --output-hashing all --configuration development && bash post-build-step.bash --include-mocks",
    "build-client-prod": "bash make-version-txt.bash build-client-prod && rm -rf dist dist.tgz && ng build --aot --output-hashing all --no-source-map --no-named-chunks && bash post-build-step.bash --include-mocks",
    "start-client-dev": "ng serve",
    "start-client-prod": "ng serve --aot",
    "post-build-step--setup-bms-for-dev": "sh build-app-for-dev.sh 0 bms git-workspace-all ${USER}",
    "test": "ng test",
    "test-headless-once": "ng test --browsers ChromeHeadless --watch=false",
    "lint--not-tested": "ng lint",
    "e2e--not-tested": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.6",
    "@angular/cdk": "^13.3.6",
    "@angular/common": "^13.3.6",
    "@angular/compiler": "^13.3.6",
    "@angular/core": "^13.3.6",
    "@angular/forms": "^13.3.6",
    "@angular/material": "13.3.6",
    "@angular/platform-browser": "^13.3.6",
    "@angular/platform-browser-dynamic": "^13.3.6",
    "@angular/router": "^13.3.6",
    "@babel/core": "^7.0.0-bridge.0",
    "@babel/polyfill": "^7.6.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.1",
    "core-js": "^2.6.10",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "find-process": "^1.4.2",
    "jsontoxml": "^1.0.1",
    "karma-coverage": "^2.2.0",
    "microtime": "^3.0.0",
    "moment": "^2.24.0",
    "ng-packagr": "^13.3.0",
    "ngx-socket-io": "^3.0.1",
    "request": "^2.88.0",
    "rxjs": "^6.6.7",
    "sha1": "^1.1.1",
    "then-request": "^6.0.2",
    "tslib": "^2.0.0",
    "zeromq": "^6.0.0-beta.6",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.6",
    "@angular/cli": "^13.3.6",
    "@angular/compiler-cli": "^13.3.8",
    "@angular/language-service": "^13.3.8",
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@types/express": "^4.17.1",
    "@types/jasmine": "2.8.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "babel-core": "^7.0.0-bridge.0",
    "codelyzer": "^6.0.2",
    "fast-xml-parser": "^3.14.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "serve-static": "^1.14.1",
    "ts-node": "^7.0.1",
    "tslint": "~6.1.0",
    "tslint-loader": "^3.5.4",
    "typescript": "4.6.2",
    "webpack": "4.0.0",
    "webpack-cli": "3.3.4"
  },
  "eslintConfig": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "eslint-plugin-typescript"
    ]
  }
}
