<?xml version="1.0" encoding="utf-8"?>
<!--
reference response to WINDOW_GRID_TEMPLATE GET command
ex: ~/cgi-bin/cgicmd?GET=WINDOW_GRID_TEMPLATE&CGI=sqreader...
-->
<gridTemplate>
  <item id="a" type="STRING" hint="Battery number" >Battery</item>
  <item id="b" type="TIMESTAMP" hint="Milliseconds Since Jan 1st" >Charge Start TS</item>
  <item id="c" type="FLOAT" hint="Total charge amp hours" >Charge AH</item>
  <item id="d" type="FLOAT" hint="Rate of discharge" >Discharge rate kW</item>
  <item id="e" type="PERCENT" hint="Total power efficiency percentage" >Full Discharge Pwr Eff</item>
  <item id="f" type="PERCENT" hint="Total AH efficiency percentage" >Full Discharge Coul Eff</item>
  <item id="g" type="FLOAT" hint="Total charge energy kWh" >Charge kWhr</item>
  <item id="h" type="FLOAT" hint="Total discharge energy kWh" >Full Discharge kWhr</item>
  <item id="i" type="FLOAT" hint="Max battery voltage" >Max Volts</item>
  <item id="j" type="FLOAT" hint="Duration of full discharge" >Full Discharge Minutes</item>
  <item id="k" type="PERCENT" hint="Power efficiency percentage stage 1" >Const Pwr Eff</item>
  <item id="l" type="FLOAT" hint="Discharge energy kWh stage 1" >Const Pwr Disharge kWhr</item>
  <item id="m" type="FLOAT" hint="Duration of constant power discharge" >Const Pwr Minutes</item>
</gridTemplate>